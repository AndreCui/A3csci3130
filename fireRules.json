{
  "rules": {
    ".read": "true",
    ".write": "true",
    "contacts": {
            "$contact": {
                "address": {
                    ".validate": "newData.isString() && newData.val().length < 50"
                },
                "businessnumber": {
                    ".validate": "newData.isString() && newData.val().matches(/[0-9]{9}$/)"
                },
              "primarybusiness": {
                ".validate": "newData.isString() && newData.val().matches(/^Fisher|Distributor|Processor|Fish Monger$/)"
              },
              "province": {
                ".validate": "newData.isString() && newData.val().matches(/^AB|BC|MB|NB|NL|NU|NS|ON|PE|QC|SK|YT| $/)"
              },
              "name": {
                ".validate": "newData.isString() && newData.val().length >1 && newData.val().length <49"
              }
            }
        }
  }
}